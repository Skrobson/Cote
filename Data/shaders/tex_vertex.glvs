#version 330 core
layout (location = 0) in vec3 position;
layout (location = 1) in vec2 uv;

out vec2 textureCoord;

void main()
{
    gl_Position = vec4(position, 1.0f);
    textureCoord =uv;//SOIL texture is flipped upside-down, for most case textureCoord=inTextureCoord; vec2(inTexCoord.x,1.0f-inTexCoord.y)
}